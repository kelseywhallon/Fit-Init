<form action="/friday" method="GET">
    <div class="card workout-card" style="width: 22rem;">
        <div class="card-header">
            <h1>Friday</h1> 
            Legs
        </div>
        <ul class="list-group list-group-flush">
            <% exercise.results.forEach(function(exercise){ %>
        <% if((exercise.language === 2) && (exercise.category === 9 || exercise.category === 14) && (exercise.id === 776 )){ %>
        <li class="list-group-item"><%=exercise.name%> x 30s</li>
        <% } %>
        <% if((exercise.language === 2) && (exercise.category === 9 || exercise.category === 14) && (exercise.id === 320 )){ %>
        <li class="list-group-item"><%=exercise.name%> x 30s</li>
        <% } %>
        <% if((exercise.language === 2) && (exercise.category === 9 || exercise.category === 14) && (exercise.id === 160 )){ %>
        <li class="list-group-item"><%=exercise.name%> x 30s</li>
        <% } %>
        <% if((exercise.language === 2) && (exercise.category === 9 || exercise.category === 14) && (exercise.id === 408 )){ %>
        <li class="list-group-item"><%=exercise.name%> x 30s</li>
        <% } %>
        <% if((exercise.language === 2) && (exercise.category === 9 || exercise.category === 14) && (exercise.id === 185 )){ %>
        <li class="list-group-item"><%=exercise.name%> x 30s</li>
        <% } %>
    </ul>
    <% }) %>
    </div>
</form>